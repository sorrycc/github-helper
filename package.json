{
  "entry": {
    "index": "./entry/index.jsx"
  },
  "dependencies": {
    "antd": "~0.10.0-beta8",
    "moment": "~2.10.6",
    "object-assign": "~4.0.1",
    "react": "~0.14.1",
    "react-dom": "~0.14.1",
    "react-redux": "~4.0.0",
    "react-spinkit": "~1.1.4",
    "redux": "~3.0.4",
    "redux-logger": "~2.0.4",
    "redux-thunk": "~1.0.0",
    "whatwg-fetch": "~0.10.0"
  },
  "devDependencies": {
    "antd-bin": "~0.10.0",
    "babel-jest": "~5.3.0",
    "jest-cli": "~0.7.1",
    "redux-mock-store-jest": "0.0.2"
  },
  "scripts": {
    "server": "antd server -p 8000",
    "dev": "antd build --debug --watch -o ./OSX/app/",
    "test": "jest",
    "copy": "cp ./*.html ./OSX/app/"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "test.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testPathDirs": [
      "<rootDir>/actions"
    ],
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/redux-mock-store-jest",
      "./node_modules/redux",
      "./node_modules/expect"
    ],
    "verbose": true
  }
}
